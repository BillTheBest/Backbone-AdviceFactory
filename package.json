  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dataminr/Backbone-AdviceFactory.git"
  },
  "keywords": [
    "factory",
    "backbone",
    "advice",
    "mixins",
    "javascript",
    "mixin",
    "aspect",
    "reuse",
    "composable"
  ],
